% \subsubsection{Footnote setup ftnright}
%
% In this section we provide the code for running all footnotes in the
% last column.
%
% \begin{macro}{\saved@footins}
%
%    \begin{macrocode}
\box_new:N \saved@footins
%    \end{macrocode}
% \end{macro}
%
%
%
% \begin{macro}{\rcol@footinsskip}
%    \begin{macrocode}
\skip_new:N \rcol@footinsskip
%    \end{macrocode}
% \end{macro}
%
%
%
%
%
% \begin{macro}{\xor_initialise_footins_ftnright:}
%    \begin{macrocode}
\def \xor_initialise_footins_ftnright: {
  \box_gclear:N \saved@footins
  \pref_global:D \skip\footins\c_zero
  \int_gzero:N {\count\footins}
  \pref_global:D \dimen\footins\textheight
}
%    \end{macrocode}
% \end{macro}
%
%
%
%
%
% \begin{macro}{\xor_deal_with_footins_ftnright:}
%    \begin{macrocode}
\def_new:Npn \xor_deal_with_footins_ftnright: {
%<*trace>
  \@tracepush{xor_deal_with_footins_ftnright:}
%</trace>
%    \end{macrocode}
%    If we are not in the last column we save any footnote accumulated
%    in |\footins| in a separate |\saved@footins| box. This way any
%    later code that checks |\footins| for constraints will not fire
%    incorrectly.
%    \begin{macrocode}
  \int_compare:nNnT\g_xor_curr_col_int<\g_xor_cols_int
  {
%<*trace>
    \tr@ce{not~ last~ col}
%</trace>
    \box_if_empty:NF \footins
    {
      \vbox_gset:Nn \saved@footins
        {\vbox_unpack_and_clear:N\saved@footins
              \vbox_unpack_and_clear:N\footins}
    }
%    \end{macrocode}
%    If we are in the second last column we have to change the
%    procedure: we now have to return all saved footnotes to the MVL
%    and modify the insertion register values so that from now on
%    footnotes need space in the column.\footnote{Not correctly
%    handled is the case that we have accumulated more than a full
%    column of footnotes before!}
%    \begin{macrocode}
    \count@\g_xor_cols_int
    \int_decr:N \count@
    \int_compare:nNnT \g_xor_curr_col_int=\count@ % cur col = second last col
    {
%<*trace>
       \tr@ce{second~ last~ col}
%</trace>
       \pref_global:D\count\footins\c_thousand
       \pref_global:D \skip\footins\rcol@footinsskip
       \box_if_empty:NF \saved@footins
       {
         \insert\footins{\vbox_unpack_and_clear:N \saved@footins}
       }
    }
  }
%<*trace>
  \@tracepop{xor_deal_with_footins_ftnright:}
%</trace>
}
%    \end{macrocode}
% \end{macro}
%
%
%
%
% \begin{macro}{\xor_deal_with_footins_margin:}
%    \begin{macrocode}
\def_new:Npn \xor_deal_with_footins_margin: {
%<*trace>
  \@tracepush{xor_deal_with_footins_margin:}
%</trace>
  \box_if_empty:NF\footins
  {
      \vbox_gset:Nn \saved@footins
           {\vbox_unpack_and_clear:N\saved@footins
            \vbox_unpack_and_clear:N\footins}
  }
%<*trace>
  \@tracepop{xor_deal_with_footins_margin:}
%</trace>
}
%    \end{macrocode}
% \end{macro}
%
%
%
%
% \begin{macro}{\xor_deal_with_footins_margin:}
%    \begin{macrocode}
\def \initialise@footins@margin {
  \box_gclear:N \saved@footins
}
%    \end{macrocode}
% \end{macro}
