% \subsection{Footnote setup}
%
% \begin{macro}{\xor_initialise_footins_action:}
%    The |\xor_initialise_footins_action:| is a hook to add code to the
%    page initialisation phase related to the footnote insertions. For
%    example, when all footnotes are accumulated in the last column we
%    need to make footnote insertions invisible when gathering
%    material for the first columns.
%    \begin{macrocode}
\let:NN \xor_initialise_footins_action:\scan_stop:
%    \end{macrocode}
% \end{macro}
%
%
% \begin{macro}{\xor_deal_with_footins_action:}
%    The |\xor_deal_with_footins_action:| hook is called in the
%    |\xor_OR_grab_trial_cols:| and the |xor_OR_grab_best_cols:| output routine to do
%    something to the accumulated footnote insertions if
%    necessary. I.e., it is called for every column.
%    \begin{macrocode}
\let:NN \xor_deal_with_footins_action:\scan_stop:
%    \end{macrocode}
% \end{macro}
%
%
%
%    \begin{macrocode}
\DeclareTemplateType{footnotesetup}{0}
%    \end{macrocode}
%
%
%
%
%    \begin{macrocode}
\DeclareTemplate{footnotesetup}{std}{0}{
   text-sep    =+l \skip\footins,
   max-height  =+l \dimen\footins,
 }
 {

  \pref_global:D \skip\footins 10pt plus 5pt
  \pref_global:D \dimen\footins.7\textheight

  \DoParameterAssignments

  \let:NN \xor_initialise_footins_action:\scan_stop:
  \let:NN \xor_deal_with_footins_action:\scan_stop:

  \pref_global:D \count\footins\c_thousand

 }
%    \end{macrocode}
%
%    \begin{macrocode}
\DeclareTemplate{footnotesetup}{ftnright}{0}{
   text-sep    =+l \rcol@footinsskip,
   max-height  =+l \dimen\footins,
 }
 {

  \pref_global:D \rcol@footinsskip 10pt plus 5pt
  \pref_global:D \dimen\footins\textheight

  \DoParameterAssignments

  \let:NN \xor_initialise_footins_action:\xor_initialise_footins_ftnright:
  \let:NN \xor_deal_with_footins_action:\xor_deal_with_footins_ftnright:

 }
%    \end{macrocode}
%
%
%    Setting |max-height| as done below has not the desired effect and
%    needs investigation.\footnote{FIX!}
%
%    \begin{macrocode}
\DeclareTemplate{footnotesetup}{margin}{0}{
   max-height  =+l \dimen\footins,
 }
 {

  \pref_global:D\dimen\footins.7\textheight

  \DoParameterAssignments

  \let:NN\xor_initialise_footins_action:\initialise@footins@margin
  \let:NN\xor_deal_with_footins_action:\xor_deal_with_footins_margin:

%    \end{macrocode}
%    If the footnote do not appear within the columns we have to
%    ensure that the |\insert| does not change the page
%    calculations. This needs changes in two places:
%    \begin{macrocode}
  \int_gzero:N {\count\footins}
  \pref_global:D \skip\footins \c_zero

 }
%    \end{macrocode}
