\exp_args:No \@@_tmp:w { \c_novalue_tl }
%</core|initex>
%<*package>
\cs_new_protected:Npx \@@_normalize_type_g:w
  { \exp_not:N \@@_normalize_type_G:w { \exp_not:V \c_novalue_tl } }
%</package>
%<*core|initex>
%    \end{macrocode}
% \end{macro}
