    \fp_show:c { l_B_fp }
    \fp_set:Nn \l_A_fp { -.001 }
    \fp_set:Nn \l_B_fp { }
    \fp_show:N \l_A_fp
    \fp_show:N \l_B_fp
    \fp_set:Nn \l_A_fp { 1.23456789e20 }
