\TESTEXP { Comparing~integer~values~as~predicates~(expect~F/T/T/F) } {
  \bool_if:nTF { \int_compare_p:nNn { 17 } = { -17 }      } 
    { \TRUE } { \FALSE } \NEWLINE
  \bool_if:nTF { \int_compare_p:nNn { 17 } = { -17 + 34 } } 
    { \TRUE } { \FALSE } \NEWLINE
  \bool_if:nTF { \int_compare_p:nNn { 17 } > { -17 }      } 
    { \TRUE } { \FALSE } \NEWLINE
  \bool_if:nTF { \int_compare_p:nNn { 17 } < { -17 }      } 
    { \TRUE } { \FALSE } \NEWLINE
}

\TESTEXP { Comparing~integer~values~(expect~F/T/T/F/T/T/F/F) } {
  \int_compare:nNnTF { 17 } = { -17 }      { \TRUE } { \FALSE } \NEWLINE
  \int_compare:nNnTF { 17 } = { -17 + 34 } { \TRUE } { \FALSE } \NEWLINE
  \int_compare:nNnTF { 17 } > { -17 }      { \TRUE } { \FALSE } \NEWLINE
  \int_compare:nNnTF { 17 } < { -17 }      { \TRUE } { \FALSE } \NEWLINE
  \int_compare:nNnT { 17 } = { -17 }      { \TRUE } \NEWLINE
  \int_compare:nNnT { 17 } = { -17 + 34 } { \TRUE } \NEWLINE
  \int_compare:nNnT { 17 } > { -17 }      { \TRUE } \NEWLINE
  \int_compare:nNnT { 17 } < { -17 }      { \TRUE } \NEWLINE
  \int_compare:nNnF { 17 } = { -17 }      { \FALSE } \NEWLINE
  \int_compare:nNnF { 17 } = { -17 + 34 } { \FALSE } \NEWLINE
  \int_compare:nNnF { 17 } > { -17 }      { \FALSE } \NEWLINE
  \int_compare:nNnF { 17 } < { -17 }      { \FALSE } \NEWLINE
}

