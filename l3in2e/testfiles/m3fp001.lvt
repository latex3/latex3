\documentclass{minimal}
\input{regression-test}

\RequirePackage{expl3}

\begin{document}
\START
\AUTHOR{Frank Mittelbach}
\ExplSyntaxOn

\OMIT
\TIMO


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{1:~ new~ +~ show}{
 \fp_new:N \l_A_fp
 \fp_new:c {l_A_fp}
 \fp_new:N \g_A_fp
 \fp_new:c {l_B_fp}
 \fp_show:N \l_A_fp
 \fp_show:N \g_A_fp
 \fp_show:N \l_B_fp
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{2:~ set_from_dim}{
  \fp_set_from_dim:Nn  \l_A_fp {1pt}
  \fp_gset_from_dim:Nn \g_A_fp {1sp}
 \fp_show:N \l_A_fp
 \dimen0=1sp
 \showthe\dimen0
 \fp_show:N \g_A_fp
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{3:~ set_eq}{
 \fp_show:N \l_A_fp
 \fp_gset_eq:NN \l_A_fp \g_A_fp
 \fp_show:N \l_A_fp
 \fp_zero:N \g_A_fp  % local?
 \fp_show:N \g_A_fp
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{4:~ setting~ fps}{
 \fp_show:N \g_A_fp % still > 0?
 \fp_set:Nn \l_A_fp {123456789.123456789}
 \fp_show:N \l_A_fp
 \fp_set:Nn \l_A_fp {123456789.123456789123}
 \fp_show:N \l_A_fp
 \fp_set:Nn \l_A_fp {12345678901.12345678901}
 \fp_show:N \l_A_fp
 \fp_set:Nn \l_B_fp {12345.123456}
 \fp_gset:Nn \g_A_fp {\l_B_fp}
}


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{5:~ setting~ fps}{
 \fp_show:N \l_B_fp
 \fp_show:N \g_A_fp % now 12345.123456?
 \fp_gset:Nn \g_A_fp {-3.5} 
 \fp_show:N \g_A_fp
 \fp_set:Nn \l_B_fp {--1.}
 \fp_show:N \l_B_fp          % should give +1!
 \fp_set:Nn \l_B_fp {.3}
 \fp_show:N \l_B_fp
 \fp_set:Nn \l_B_fp {-.55}
 \fp_show:N \l_B_fp
 \fp_set:Nn \l_B_fp {-+1.5}
 \fp_show:N \l_B_fp
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{6:~ abs}{
 \fp_set:Nn \l_A_fp {-5}
 \fp_abs:N  \l_A_fp
 \fp_show:N \l_A_fp 
 \fp_abs:N  \l_A_fp
 \fp_show:N \l_A_fp 
 \fp_set:Nn \l_A_fp {6}
 \fp_abs:N  \l_A_fp
 \fp_show:N \l_A_fp 
 \fp_set:Nn \l_A_fp {--15.34}
 \fp_abs:N  \l_A_fp
 \fp_show:N \l_A_fp 
 \fp_set:Nn \l_A_fp {+-0.123}
 \fp_abs:N  \l_A_fp
 \fp_show:N \l_A_fp 
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{7:~ abs~ with~ arg}{
 \fp_abs:Nn  \l_A_fp {-1.2}
 \fp_show:N \l_A_fp 
 \fp_abs:Nn  \l_B_fp {-\l_A_fp}
 \fp_show:N \l_B_fp 
 \fp_abs:Nn  \l_A_fp {+-1.123456789123}
 \fp_show:N \l_A_fp 
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{8:~ global~ abs}{
 { \fp_gset:Nn \g_A_fp {-5} \fp_gabs:N  \g_A_fp }
 \fp_show:N \g_A_fp 
 { \fp_gabs:N  \g_A_fp }
 \fp_show:N \g_A_fp 
 {\fp_gset:Nn \g_A_fp {6} \fp_gabs:N  \g_A_fp }
 \fp_show:N \g_A_fp 
 { \fp_gset:Nn \g_A_fp {--15.34} \fp_gabs:N  \g_A_fp }
 \fp_show:N \g_A_fp 
 { \fp_gset:Nn \g_A_fp {+-0.123} \fp_gabs:N  \g_A_fp }
 \fp_show:N \g_A_fp 
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{9:~ negation}{
 \fp_set:Nn  \l_A_fp {-1.2}
 \fp_neg:N \l_A_fp
 \fp_show:N \l_A_fp
 \fp_neg:N \l_A_fp
 \fp_show:N \l_A_fp 
 \fp_set:Nn  \l_A_fp {-987654321.123456789}
 \fp_neg:N \l_A_fp
 \fp_show:N \l_A_fp
 { \fp_neg:N \l_A_fp \fp_show:N \l_A_fp }
 \fp_show:N \l_A_fp
 }


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{10:~ global~ negation}{
 { \fp_gset:Nn  \l_A_fp {-1.2} \fp_gneg:N \l_A_fp }
 \fp_show:N \l_A_fp
 { \fp_gneg:N \l_A_fp }
 \fp_show:N \l_A_fp 
 { \fp_gset:Nn  \l_A_fp {-987654321.123456789} \fp_gneg:N \l_A_fp }
 \fp_show:N \l_A_fp
 { \fp_gneg:N \l_A_fp }
 \fp_show:N \l_A_fp
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{11:~ negation~ with~ arg}{
 \fp_neg:Nn  \l_A_fp {-1.2}
 \fp_show:N \l_A_fp
 \fp_neg:Nn \l_A_fp {\l_A_fp}
 \fp_show:N \l_A_fp 
 \fp_set:Nn  \l_A_fp {-987654321.123456789}
 \fp_neg:Nn \l_A_fp {\l_A_fp}
 \fp_show:N \l_A_fp
 \fp_neg:Nn \l_A_fp {-\l_A_fp}
 \fp_show:N \l_A_fp
 { \fp_neg:Nn \l_A_fp {-5} \fp_show:N \l_A_fp }
 \fp_show:N \l_A_fp
}


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{11:~ global~ negation~ with~ arg}{
 { \fp_gneg:Nn  \g_A_fp {1.2} }
 \fp_show:N \g_A_fp
 { \fp_gneg:Nn \g_A_fp {-\g_A_fp } }
 \fp_show:N \g_A_fp 
 { \fp_gneg:Nn  \g_A_fp {-987654321.123456789} }
 \fp_show:N \g_A_fp
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{12:~ adding}{
 \fp_set:Nn  \l_A_fp {-0}
 \fp_add:Nn  \l_A_fp {-1}
 \fp_show:N \l_A_fp
 \fp_add:Nn  \l_A_fp {100}
 \fp_show:N \l_A_fp
 \fp_add:Nn  \l_A_fp {2000}
 \fp_show:N \l_A_fp
 \fp_add:Nn  \l_A_fp {-3000.123456789} \BUG
 \fp_show:N \l_A_fp
 \fp_add:Nn  \l_A_fp {40000}
 \fp_show:N \l_A_fp
}



%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TEST{13:~ more~ adding}{
 \fp_add:Nnn  \l_A_fp {-0}{-0}
 \fp_show:N \l_A_fp
 \fp_add:Nnn  \l_A_fp {12345}{-12245}
 \fp_show:N \l_A_fp
 \fp_add:Nnn  \l_A_fp {1.98765}{-1.98765}
 \fp_show:N \l_A_fp
 \fp_add:Nnn  \l_A_fp {123456789.987654321}{-987654321.123456789}
 \fp_show:N \l_A_fp
%
%
 \fp_set:Nn  \l_A_fp {99.123456}
 \fp_show:N \l_A_fp
 \fp_add:Nn  \l_A_fp {-40000} \BUG
 \fp_show:N \l_A_fp
 \fp_add:Nn  \l_A_fp {40000} \BUG
 \fp_show:N \l_A_fp
}


