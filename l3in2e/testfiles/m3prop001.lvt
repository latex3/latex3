\TEST{map_function}{
  \prop_clear:N  \l_a_prop
  \prop_put:Nnn \l_a_prop {a} {#testa\par}
  \prop_put:Nnn \l_a_prop {b} {#testb\par}
  \cs_set:Npn \propmap #1#2 {
    \cs_set_nopar:Npx \tmp {>\detokenize{#1}---\detokenize{#2}<}
    \TYPE {\tmp}
  }
  \prop_map_function:NN \l_a_prop \propmap
}

