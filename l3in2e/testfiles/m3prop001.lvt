\prop_clear:N  \l_a_prop
\prop_put:Nnn \l_a_prop {a} {testa}
\prop_put:Nnn \l_a_prop {b} {testb}
\cs_set:Npn \propmap #1#2 {>#1---#2<\NEWLINE}
  
