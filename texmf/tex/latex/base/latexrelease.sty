\def \@makecol {%
  \UseHook {build/column/before}%
  \setbox\@outputbox \box\@cclv
  \@outputbox@removebskip
  \UseTaggingSocket{build/column/outputbox}%
  \let\@elt\relax
  \xdef\@freelist{\@freelist\@midlist}%
  \global \let \@midlist \@empty
  \UseSocket {build/column/outputbox}%
  \ifvbox\@kludgeins
     \@make@specialcolbox
  \else
     \@make@normalcolbox
  \fi
  \global \maxdepth \@maxdepth
  \UseHook {build/column/after}%
}
\newdimen\@outputbox@depth
\def \@make@normalcolbox {%
   \setbox\@outputbox \vbox to\@colht {%
       \@texttop
       \@outputbox@depth \dp\@outputbox
       \unvbox \@outputbox
       \vskip -\@outputbox@depth
       \@textbottom
      }%
}
\def \@make@specialcolbox {%
   \@outputbox@append {\vskip-\@outputbox@depth}%
   \@tempdima \@colht
   \ifdim \wd\@kludgeins>\z@
     \advance \@tempdima -\ht\@outputbox
     \advance \@tempdima \pageshrink
     \setbox\@outputbox \vbox to \@colht {%
       \unvbox\@outputbox
       \vskip \@tempdima
       \@textbottom
       }%
   \else
     \advance \@tempdima -\ht\@kludgeins
     \setbox \@outputbox \vbox to \@colht {%
       \vbox to \@tempdima {%
         \unvbox\@outputbox
         \@textbottom}%
       \vss}%
   \fi
   {\setbox \@tempboxa \box \@kludgeins}%
}
\def\@outputbox@removebskip{%
   \ifvoid \@outputbox
     \global\let\@outputbox@reinsertbskip\relax
   \else
     \@outputbox@append{%
       \@tempskipa\lastskip
       \ifnum \gluestretchorder\@tempskipa>\z@
         \unskip
         \xdef\@outputbox@reinsertbskip
             {%
               \noexpand\@outputbox@append{%
                 \noexpand\@if@footnotes@TF
                     {}%
                     {%
                      \noexpand\@if@bottomfloats@TF
                         {}%
                         {\noexpand\@backup@outputbox@depth}%
                     }%
                 \vskip\the\@tempskipa}}%
       \else
         \global\let\@outputbox@reinsertbskip\relax
       \fi
     }%
  \fi
}
\let \@outputbox@reinsertbskip \relax
\def\@backup@outputbox@depth{%
  \ifdim\@outputbox@depth>\z@
    \vskip -%
      \ifdim\@outputbox@depth>\maxdepth
        \maxdepth
      \else
        \@outputbox@depth
      \fi
    \@outputbox@depth\z@
  \fi
}
\def\@outputbox@append #1{%
   \setbox\@outputbox \vbox {%
     \boxmaxdepth \@maxdepth
     \@outputbox@depth\dp\@outputbox      % if needed in #1
     \unvbox \@outputbox
     #1%
   }%
}
