  { \__kernel_backend_literal:n { pdf : bc ~ fill ~ [ #1 ] } }
\cs_new_eq:NN \@@_backend_fill_cmyk:n \@@_backend_fill:n
\cs_new_eq:NN \@@_backend_fill_gray:n \@@_backend_fill:n
\cs_new_eq:NN \@@_backend_fill_rgb:n  \@@_backend_fill:n
