%    \begin{macrocode}
\bool_new:N \g_@@_landscape_bool
%<*package>
\AtBeginDocument
  {
    \cs_if_exist:NT \landscape
      {
        \tl_put_right:Nn \landscape
          { \bool_gset_true:N \g_@@_landscape_bool }
        \tl_put_left:Nn \endlandscape
          { \bool_gset_false:N \g_@@_landscape_bool }
      }
  }
%</package>
%    \end{macrocode}
% \end{variable}
