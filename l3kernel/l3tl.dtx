\cs_new_protected:Npn \@@_map_variable:Nnn #1#2#3
  {
    \tl_set:Nn #1 {#3}
    \__quark_if_recursion_tail_break:NN #1 \tl_map_break:
    \use:n {#2}
    \@@_map_variable:Nnn #1 {#2}
