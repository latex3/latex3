          { \exp_not:N \l_@@_color_current_tl }
    \group_insert_after:N \exp_not:N \@@_color_reset:
  }
\cs_new_protected:Npx \@@_color_reset:
  {
    \cs_if_exist:NTF \luatex_pdfextension:D
      { \luatex_pdfextension:D colorstack }
