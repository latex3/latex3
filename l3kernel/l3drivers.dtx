\tl_new:N \l_@@_color_current_tl
\tl_set:Nn \l_@@_color_current_tl { 0~g~0~G }
%<*package>
\AtBeginDocument
  {
    \@ifpackageloaded { color }
      { \tl_set:Nn \l_@@_color_current_tl { \current@color } }
      { }
