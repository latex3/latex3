      \@@_pair:wn \tl_to_str:n {#2} \s_@@ { }
      \q_recursion_tail
    \__prg_break_point:
  }
\cs_new:Npn \@@_if_in:nwwn #1#2 \@@_pair:wn #3 \s_@@ #4
  {
    \str_if_eq_x:nnTF {#1} {#3}
      { \@@_if_in:N }
      { \@@_if_in:nwwn {#1} }
  }
\cs_new:Npn \@@_if_in:N #1
  {
    \if_meaning:w \q_recursion_tail #1
      \prg_return_false:
    \else:
      \prg_return_true:
    \fi:
    \__prg_break:
  }
