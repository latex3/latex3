%   }
%   \begin{syntax}
%     \cs{box_set_eq_clear:NN} \meta{box1} \meta{box2}
%   \end{syntax}
%   Sets the content of \meta{box1} within the current \TeX{} group
%   equal to that of \meta{box2}, then clears \meta{box2} globally.
% \end{function}
%
% \begin{function}
%   {
%     \box_gset_eq_clear:NN, \box_gset_eq_clear:cN,
