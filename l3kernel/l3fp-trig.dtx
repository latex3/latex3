  }
\cs_new:Npn \@@_atan_near:wwwn
    0 \@@_ep_div:wwwwn #1,#2; #3,
  {
    1
    \@@_ep_to_fixed:wwn #1 - #3, #2;
    \@@_atan_near_aux:wwn
  }
\cs_new:Npn \@@_atan_near_aux:wwn #1; #2;
  {
    \@@_fixed_add:wwn #1; #2;
    { \@@_fixed_sub:wwn #2; #1; { \@@_ep_div:wwwwn 0, } 0, }
