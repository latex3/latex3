%   representation of~$z$ and the old representation.
%    \begin{macrocode}
\cs_new:Npn \@@_atan_auxi:ww #1,#2;
  { \@@_ep_to_fixed:wwn #1,#2; \@@_atan_auxii:w #1,#2; }
