                  {
                    \clist_map_inline:nn {##1}
                      {
                        \tl_put_right:Nn \l_@@_tmpa_tl
                          { ^^J\space\space\space\space ####1 }
                      }
                  }
                \int_set:Nn \l_tmpa_int { \etex_interactionmode:D }
                \errorstopmode
                \ClassError{l3doc}{\l_@@_tmpa_tl}{}
                \int_set:Nn \etex_interactionmode:D {\l_tmpa_int}
              }
          }
      }
  }
