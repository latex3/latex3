% \begin{macro}{\@@_print_testfile:n}
%   Used to show that a macro has a test, somewhere.
%    \begin{macrocode}
\DeclareDocumentCommand \@@_print_testfile:n {m}
  {
    \bool_set_true:N \l_@@_tested_bool
