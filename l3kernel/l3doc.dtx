% \begin{macro}{\cmd, \cs}
% \begin{macro}[aux]{\@@_cs:n, \@@_cmd:n}
%   |\cmd{\foo}| Prints |\foo| verbatim. It may be used inside moving
%   arguments. |\cs{foo}| also prints |\foo|, for those who prefer that
%   syntax.  There is a bit of shuffling around to make sure that |_|
%   has the catcode that \cs{@@_replace_at_at:N} expects, without
%   rescanning the whole argument.  For that we store in
%   \cs{l_@@_tmpb_tl} a single underscore with the current catcode, then
%   replace all such underscores by letter underscores in |#1| (stored
%   in the |tmpa| token list).  After calling \cs{@@_replace_at_at:N},
%   we replace the underscores back to their old meaning before using
%   the token list.  Also replace all spaces by \tn{@xobeysp} and set
%   the font with \tn{verbatim@font}.
%    \begin{macrocode}
\DeclareDocumentCommand \cmd {m} { \@@_cmd:n { #1 } }
\DeclareDocumentCommand \cs  {m} { \@@_cs:n  { #1 } }
\cs_new_protected:Npn \@@_cs:n #1
  { \exp_args:Nf \@@_cmd:n { \iow_char:N \\ #1 } }
\cs_new_protected:Npn \@@_cmd:n #1
  {
    \tl_set_rescan:Nnn \l_@@_tmpb_tl { } { _ }
    \tl_set:No \l_@@_tmpa_tl { \token_to_str:N #1 }
    \tl_replace_all:NVn \l_@@_tmpa_tl \l_@@_tmpb_tl { _ }
    \@@_replace_at_at:N \l_@@_tmpa_tl
    \tl_replace_all:NnV \l_@@_tmpa_tl { _ } \l_@@_tmpb_tl
    \tl_replace_all:Nnn \l_@@_tmpa_tl { ~ } \@xobeysp
    \mode_if_math:TF
      { \mbox { \verbatim@font \l_@@_tmpa_tl } }
      {       { \verbatim@font \l_@@_tmpa_tl } }
    \@@_special_main_index:o { \l_@@_tmpa_tl }
  }
\cs_generate_variant:Nn \tl_replace_all:Nnn { NV , NnV }
%    \end{macrocode}
% \end{macro}
% \end{macro}
%
% \begin{macro}{\tn}
%   Similar to \cs{cs}, this is used to mark up all \TeX{} and \LaTeXe{}
%   commands; \cs{@@_special_sub_index:nnn} makes sure that they end up
%   together in the index.  To get \cs{tn} to work inside bookmarks, we
%   declare it to be expandable and to expand to a \cs{texorpdfstring}
%   construction.  Inside a bookmark we just produce an escape character
%   and~|#2|.  Outside, we call a protected command two spaces at the
%   end of its name: this is to avoid possible collisions with internal
%   code of \cs{DeclareExpandableDocumentCommand}.  This command takes
%   care of typesetting, which is simpler than for \cs{cs} since there
%   is no need for the |@@| replacement.  The auxiliary is named this
%   way so that when \cs{tn} is written to a file and read back it gives
%   \cs{tn} again.
%    \begin{macrocode}
\DeclareExpandableDocumentCommand \tn { m }
  {
    \texorpdfstring
      { \use:c { tn \c_space_tl \c_space_tl } {#1} }
      { \iow_char:N \\ #1 }
  }
\cs_new_protected:cpn { tn \c_space_tl \c_space_tl } #1
  {
    \texttt { \char`\\ #1 }
    \exp_args:Nf \@@_special_sub_index:nnn { \iow_char:N \\ #1 }
      { TeX~and~LaTeX2e~commands }
      { \string\TeX{}~and~\string\LaTeXe{}~commands: }
  }
%    \end{macrocode}
% \end{macro}
