              {
                \tl_put_right:Nn \l_@@_tmpa_tl { ##1 ^^J }
                \iow_now:Nn \g_@@_func_iow { ? ~ ##1}
              }
          }
        \@@_functions_typeout:n
          { Functions~defined~but~not~documented: }

        \iow_close:N \g_@@_func_iow
