\clist_new:N \l_@@_macro_input_clist
\cs_new_nopar:Npn \@@_macro_aux:n #1
  {
    \group_end:
    \clist_set:Nn \l_@@_macro_input_clist {#1}
    \bool_if:NTF \l_@@_macro_pTF_bool
      {
