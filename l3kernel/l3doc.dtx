        \bool_set_true:N \l_@@_macro_TF_bool
        \clist_map_function:NN \l_@@_macro_input_clist
          \@@_macro_single:n
        \bool_set_false:N \l_@@_macro_TF_bool
      }
      {
        \clist_map_function:NN \l_@@_macro_input_clist
          \@@_macro_single:n
      }
