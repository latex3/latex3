% \begin{macro}{\@@_meta_orig:n}
%   Straight from \pkg{doc.sty}.
%    \begin{macrocode}
\cs_set_protected:Npn \@@_meta_orig:n #1
  {
    \ensuremath\langle
    \ifmmode \expandafter \nfss@text \fi
    {%
      \meta@font@select
      \edef\meta@hyphen@restore
        {\hyphenchar\the\font\the\hyphenchar\font}%
      \hyphenchar\font\m@ne
      \language\l@nohyphenation
      #1\/%
      \meta@hyphen@restore
    }\ensuremath\rangle
  }
%    \end{macrocode}
% \end{macro}
%
% \end{macro} %^^A% end macro: {meta}
