    TF .code:n =
      { \bool_set_true:N \l_@@_macro_TF_bool } ,
    pTF .code:n =
      { \bool_set_true:N \l_@@_macro_pTF_bool } ,
    tested .code:n =
      { } ,
    EXP .code:n = {} , % TODO
