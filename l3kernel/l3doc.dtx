% \begin{macro}{\@@_close_meta:n, \@@_open_meta:w, \@@_close_meta:w}
%    \begin{macrocode}
\cs_new_nopar:Npn \@@_close_meta:n #1
  { \@@_meta_orig:n {#1} \group_end: }
\cs_new_nopar:Npn \@@_close_meta:w #1 >
  { \@@_meta_orig:n {#1} \group_end: }
\cs_new:Npn \@@_open_meta:w
  {
