% \begin{macro}{\Arg}
%   Once we switch from active chars to replacing for the underscore
%   trick, this function will then be the same as \cs{marg}.  (I have a
%   feeling that in the distant past there were some additional spacing
%   effects in \cs{Arg} for use inside \texttt{function}.  To explain
%   why we have both \cs{Arg} and \cs{marg}.)
%    \begin{macrocode}
\cs_new:Npn \Arg
  {
    \texttt{ \char`\{ }
    \group_begin:
      \char_set_active_eq:NN _ \@@_ensuremath_sb:n
      \@@_close_Arg:n
  }
%    \end{macrocode}
%    \begin{macrocode}
\cs_new_nopar:Npn \@@_close_Arg:n #1
  {
    \@@_meta_orig:n {#1}
    \group_end:
    \texttt{ \char`\} }
  }
%    \end{macrocode}
