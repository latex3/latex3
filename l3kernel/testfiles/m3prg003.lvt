\TESTEXP{Eager~evaluation}{
  \bool_if_p:n{
    \int_compare_p:nNn 1=1 &&
    \bool_if_p:n {
      \int_compare_p:nNn 2=3 ||
      \int_compare_p:nNn 4=4 ||
      \int_compare_p:nNn 1={2\expectedError} % is not skipped any longer
    } &&
    \int_compare_p:nNn 2=2
  }
}

