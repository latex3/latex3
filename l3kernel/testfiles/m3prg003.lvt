  \bool_if:nTF { (! \c_true_bool)   }{fail}{pass}  \NEWLINE
  \bool_if:nTF { (! \c_false_bool)  }{pass}{fail}  \NEWLINE
  \bool_if:nTF { !(! \c_true_bool)   }{pass}{fail}  \NEWLINE
  \bool_if:nTF { !(! \c_false_bool)  }{fail}{pass}  \NEWLINE
  \bool_if:nTF { !(! (\c_true_bool))   }{pass}{fail}  \NEWLINE
  \bool_if:nTF { !(! (\c_false_bool))  }{fail}{pass}  \NEWLINE

}

