  {
    \str_case:nnF { \tmp }
      {
        { x }      { \NO }
        { y }      { \NO }
        { yz }     { \NO }
        { \ERROR } { \ERROR }
      }
      { \YES }
    \NEWLINE
    \str_case:nnF { yz }
      {
        { x }      { \NO }
        { y }      { \NO }
        { yz }     { \YES }
        { \ERROR } { \ERROR }
      }
      { \NO }
    \NEWLINE
    \str_case:nnF { x\tmp }
      {
        { x }      { \NO }
        { y }      { \NO }
        { yz }     { \NO }
        { \ERROR } { \ERROR }
      }
      { \YES }
    \NEWLINE
    \str_case:nnF { abc }
      { { x } { \NO } }
      { \YES }
    \NEWLINE
    \str_case_e:nnF { \tmp }
      {
        { x }      { \NO }
        { y }      { \NO }
        { yz }     { \YES }
        { \ERROR } { \ERROR }
      }
      { \NO }
    \NEWLINE
    \str_case_e:nnF { yz }
      {
        { x }      { \NO }
        { y }      { \NO }
        { yz }     { \YES }
        { \ERROR } { \ERROR }
      }
      { \NO }
    \NEWLINE
    \str_case_e:nnF { x\tmp }
      {
        { x }      { \NO }
        { y }      { \NO }
        { xyz }     { \YES }
        { \ERROR } { \ERROR }
      }
      { \NO }
