\TEST{seq_get}{
  \seq_clear:N     \l_tmpa_seq

  \seq_get:NN      \l_tmpa_seq \l_tmpa_tl
  \tl_log:N \l_tmpa_tl

  \seq_put_left:Nn \l_tmpa_seq {}
  \seq_get:NN      \l_tmpa_seq \l_tmpa_tl
  \tl_log:N \l_tmpa_tl

  \seq_put_left:Nn \l_tmpa_seq {a\par}
  {
    \seq_get:NN    \l_tmpa_seq \l_tmpa_tl
    \tl_log:N \l_tmpa_tl
  }
  \tl_log:N \l_tmpa_tl

  \seq_put_left:Nn \l_tmpa_seq {b\par}
  \seq_get:NN      \l_tmpa_seq \l_tmpa_tl
  \tl_log:N \l_tmpa_tl
}

