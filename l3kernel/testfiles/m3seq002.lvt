  \seq_gput_left:Nn \g_b_seq { test~ b }
  {
    \seq_clear:N \l_a_seq
    \seq_gclear:N \g_b_seq
    \TYPE { \cs_meaning:N \l_a_seq }
  }
  \TYPE { \cs_meaning:N \l_a_seq }
  \TYPE { \cs_meaning:N \g_b_seq }
  \SEPARATOR
  {
    \TYPE { \cs_meaning:N \l_undefined_seq }
    \TYPE { \cs_meaning:N \g_undefined_seq }
