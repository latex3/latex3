\TEST { .cs_set:Np }
  {
    \keys_define:nn { module }
      {
        key-one .cs_set:Np  = \my_local_cs:nn #1#2 ,
        key-two .cs_gset:Np = \my_global_cs:nn #1#2
      }
    \cs_show:N \my_local_cs:nn
    \cs_show:N \my_global_cs:nn
    \group_begin:
      \keys_set:nn { module }
        {
          key-one   = #1~and~#2 ,
          key-two   = (#1)(#2)
        }
      \cs_show:N \my_local_cs:nn
      \cs_show:N \my_global_cs:nn
    \group_end:
    \cs_show:N \my_local_cs:nn
    \cs_show:N \my_global_cs:nn
  }

