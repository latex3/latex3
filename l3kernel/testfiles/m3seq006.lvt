\TEST { branching~get }
  {
    \seq_get:NNTF \l_tmpa_seq \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
      \seq_push:Nn \l_tmpa_seq { }
    \TIMO
    \seq_get:NNTF \l_tmpa_seq \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
      \seq_push:Nn \l_tmpa_seq { a }
    \TIMO
    \seq_get:NNTF \l_tmpa_seq \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
      \seq_push:Nn \l_tmpa_seq { b\par }
    \TIMO
    \seq_get:NNTF \l_tmpa_seq \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
  }

