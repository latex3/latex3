\OMIT
\cs_set:Npn \test:n #1
  {
    \tl_if_head_is_N_type:nT {#1} {N}
    \tl_if_head_is_group:nT  {#1} {G}
    \tl_if_head_is_space:nT  {#1} {S}
    \tl_if_empty:nT       {#1} {E}
    /
  }
\TIMO

\TESTEXP{tl_if_head_is_group/N_type/space}{
  \test:n {}
  \test:n {~}
  \exp_args:Nf \test:n { \prg_replicate:nn {5} {~} }
  \test:n {\foo}
  \test:n {^^65}
  \test:n {~\foo}
  \test:n { \fi: ~ }
  \test:n { ~*~ }
  \test:n { \if: \fi: }
  \test:n { { } }
  \test:n { { }~}
  \test:n {~{ } }
  \test:n {~{ }~}
  \test:n { {A} }
  \test:n { {A}~}
  \test:n {~{A} }
  \test:n {~{A}~}
  \test:n {\if_false: { \fi: } }
  \test:n { {B}A}
  \test:n {{} {}}
}

