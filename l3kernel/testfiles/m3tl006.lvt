\TESTEXP{str_head}{
  \str_if_eq:eeF { \str_head:n {} }    { } ?
  \str_if_eq:eeF { \str_head:n {~} }   { ~ } ?
  \str_if_eq:eeF
    { \exp_args:Nf \str_head:n { \prg_replicate:nn {5} {~} } }
    { ~ } ?
  \str_if_eq:eeF { \str_head:n {^^65}   } { e }              ?
  \str_if_eq:eeF { \str_head:n {~\foo}  } { ~ }              ?
  \str_if_eq:eeF { \str_head:n { \* ~ } } { \iow_char:N \\ } ?
  \str_if_eq:eeF { \str_head:n { ~*~ }  } { ~ }              ?
  \str_if_eq:eeF { \str_head:n { A B }  } { A }              ?
  \str_if_eq:eeF { \str_head:n { { } }  } { \iow_char:N \{ } ?
}

