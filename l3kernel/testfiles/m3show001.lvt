\documentclass{minimal}
\input{regression-test}
\RequirePackage[log-functions]{expl3}

\begin{document}

\START
\AUTHOR{Will Robertson}
\ExplSyntaxOn

\OMIT  % these show register numbers screwing tests up if other module
       % define additional ones
  \int_new:N  \myint
  \dim_new:N  \mydim
  \skip_new:N  \myskip
  \tl_new:N  \mytl
  \prop_new:N   \myprop
  \seq_new:N   \myseq
  \clist_new:N   \myclist
\TIMO

\TEST{INTEGER}{
  \int_incr:N \myint
  \int_show:N \myint}

\TEST{DIMENSION}{
  \dim_set:Nn \mydim {1pt}
  \dim_show:N \mydim}

\TEST{SKIP}{
  \skip_set:Nn \myskip {1pt plus 2pt minus 3pt}
  \skip_show:N \myskip}

\TEST{TL}{
  \tl_set:Nn \mytl {tokens}
  \tl_show:N \mytl}

\TEST{SEQ}{
  \seq_push:Nn \myseq {item1}
  \seq_push:Nn \myseq {item2}
  \seq_show:N  \myseq
  \seq_show:c {myseq}}

\TEST{CLIST}{
  \clist_set:Nn \l_my_very_very_very_very_very_very_long_clist
    { item2 , item1 }
  \clist_show:N \l_my_very_very_very_very_very_very_long_clist
  \clist_show:c { l_my_very_very_very_very_very_very_long_clist } }

\TEST{PROP}{
  \prop_put:Nnn \myprop {key1} {val1}
  \prop_put:Nnn \myprop {key2} {val2}
  \prop_show:N  \myprop
  \prop_show:c {myprop}}

\TEST{IO}{
  \ior_list_streams:
  \iow_list_streams:
}

\TEST { BOOL }
  {
    \bool_show:N \myboold
    \bool_new:N  \myboold
    \bool_show:N \myboold
    \bool_set_true:N \myboola
    \bool_show:N \myboola
    \bool_show:N \c_true_bool
    \bool_show:N \c_false_bool
    \bool_show:n
      { \c_true_bool && !( \int_compare_p:n { 1=2 } || \c_false_bool ) }
    \bool_show:n
      { \c_true_bool && !( \int_compare_p:n { 1=2 } || \c_true_bool ) }
  }

\END
