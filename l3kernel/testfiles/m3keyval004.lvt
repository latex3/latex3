      {
        \tex_halign:D
          {
            \TYPE{0} ## \TYPE{1} \tex_cr:D
            \scan_stop: \keyval_parse:nnn {#1} {#2} { a, a=b&c }
            \tex_cr:D
          }
      }
  }
\TIMO

\test:nn \use_none:n \use_none:nn
