    \TIMO
    \clist_pop:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
  }

\TEST { branching~gpop }
  {
    \group_begin:
      \clist_gpop:NNTF \g_tmpa_clist \l_tmpa_tl \TRUE \FALSE
      \tl_log:N \l_tmpa_tl
    \group_end:
    \tl_log:N \l_tmpa_tl
    \clist_log:N \g_tmpa_clist
    \OMIT
      \clist_gpush:Nn \g_tmpa_clist { }
    \TIMO
    \group_begin:
      \clist_gpop:NNTF \g_tmpa_clist \l_tmpa_tl \TRUE \FALSE
      \tl_log:N \l_tmpa_tl
    \group_end:
    \tl_log:N \l_tmpa_tl
    \clist_log:N \g_tmpa_clist
    \OMIT
      \clist_gpush:Nn \g_tmpa_clist { a }
    \TIMO
    \group_begin:
      \clist_gpop:NNTF \g_tmpa_clist \l_tmpa_tl \TRUE \FALSE
      \tl_log:N \l_tmpa_tl
    \group_end:
    \tl_log:N \l_tmpa_tl
    \clist_log:N \g_tmpa_clist
    \OMIT
      \clist_gpush:Nn \g_tmpa_clist { b\par }
    \TIMO
    \group_begin:
      \clist_gpop:NNTF \g_tmpa_clist \l_tmpa_tl \TRUE \FALSE
      \tl_log:N \l_tmpa_tl
    \group_end:
    \tl_log:N \l_tmpa_tl
    \clist_log:N \g_tmpa_clist
  }

