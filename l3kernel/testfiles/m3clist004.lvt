\documentclass{minimal}
\input{regression-test}

\RequirePackage[check-declarations,log-functions]{expl3}

\begin{document}
\START
\AUTHOR{Joseph Wright}
\ExplSyntaxOn

\TEST { branching~get }
  {
    \clist_get:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_show:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { }
    \TIMO
    \clist_get:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_show:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { a }
    \TIMO
    \clist_get:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_show:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { b\par }
    \TIMO
    \clist_get:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_show:N \l_tmpa_tl
  }
  
\TEST { branching~pop }
  {
    \clist_pop:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_show:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { }
    \TIMO
    \clist_pop:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_show:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { a }
    \TIMO
    \clist_pop:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_show:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { b\par }
    \TIMO
    \clist_pop:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_show:N \l_tmpa_tl
  }
  
\TEST { branching~gpop }
  {
    \group_begin:
      \clist_gpop:NNTF \g_tmpa_clist \l_tmpa_tl \TRUE \FALSE
      \tl_show:N \l_tmpa_tl
    \group_end:
    \tl_show:N \l_tmpa_tl
    \clist_show:N \g_tmpa_clist
    \OMIT
      \clist_gpush:Nn \g_tmpa_clist { }
    \TIMO
    \group_begin:
      \clist_gpop:NNTF \g_tmpa_clist \l_tmpa_tl \TRUE \FALSE
      \tl_show:N \l_tmpa_tl
    \group_end:
    \tl_show:N \l_tmpa_tl
    \clist_show:N \g_tmpa_clist
    \OMIT
      \clist_gpush:Nn \g_tmpa_clist { a }
    \TIMO
    \group_begin:
      \clist_gpop:NNTF \g_tmpa_clist \l_tmpa_tl \TRUE \FALSE
      \tl_show:N \l_tmpa_tl
    \group_end:
    \tl_show:N \l_tmpa_tl
    \clist_show:N \g_tmpa_clist
    \OMIT
      \clist_gpush:Nn \g_tmpa_clist { b\par }
    \TIMO
    \group_begin:
      \clist_gpop:NNTF \g_tmpa_clist \l_tmpa_tl \TRUE \FALSE
      \tl_show:N \l_tmpa_tl
    \group_end:
    \tl_show:N \l_tmpa_tl
    \clist_show:N \g_tmpa_clist
  }

\END
