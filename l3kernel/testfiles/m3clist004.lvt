\AUTHOR{Joseph Wright}
\ExplSyntaxOn

\TEST { branching~get }
  {
    \clist_get:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { }
    \TIMO
    \clist_get:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { a }
    \TIMO
    \clist_get:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { b\par }
    \TIMO
    \clist_get:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
  }

\TEST { branching~pop }
  {
    \clist_pop:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { }
    \TIMO
    \clist_pop:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
      \clist_push:Nn \l_tmpa_clist { a }
    \TIMO
    \clist_pop:NNTF \l_tmpa_clist \l_tmpa_tl \TRUE \FALSE
    \tl_log:N \l_tmpa_tl
    \OMIT
