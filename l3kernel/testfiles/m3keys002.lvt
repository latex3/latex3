\TEST { .tl_set:N }
  {
    \OMIT
      \tl_set:Nn \l_tmpb_tl { foo }
    \TIMO
    \keys_define:nn { module }
      {
        key-one   .tl_set:N  = \l_tmpa_tl ,
        key-two   .tl_set:N  = \l_mya_tl  ,
        key-three .tl_gset:N = \g_tmpa_tl ,
        key-four  .tl_gset:N = \g_mya_tl  ,
      }
    \group_begin:
      \keys_set:nn { module }
        {
          key-one   = \par ,
          key-two   = # stuff ,
          key-three = \l_tmpb_tl ,
          key-four  = bar ,
        }
      \tl_show:N \l_tmpa_tl
      \tl_show:N \l_mya_tl
      \tl_show:N \g_tmpa_tl
      \tl_show:N \g_mya_tl
    \group_end:
    \tl_show:N \l_tmpa_tl
    \tl_show:N \l_mya_tl
    \tl_show:N \g_tmpa_tl
    \tl_show:N \g_mya_tl
  }
  
