\TEST { .bool_set:N }
  {
    \keys_define:nn { module }
      {
        key-one   .bool_set:N  = \l_tmpa_bool ,
        key-two   .bool_set:N  = \l_mya_bool  ,
        key-three .bool_gset:N = \g_tmpa_bool ,
        key-four  .bool_gset:N = \g_mya_bool  ,
      }
    \group_begin:
      \keys_set:nn { module }
        {
          key-one   = true ,
          key-two   = false ,
          key-three = false ,
          key-four  = true ,
        }
      \bool_show:N \l_tmpa_bool
      \bool_show:N \l_mya_bool
      \bool_show:N \g_tmpa_bool
      \bool_show:N \g_mya_bool
    \group_end:
    \bool_show:N \l_tmpa_bool
    \bool_show:N \l_mya_bool
    \bool_show:N \g_tmpa_bool
    \bool_show:N \g_mya_bool
  }
  
