\TESTEXP{Division~special~cases}{
  \TYPE {Expect~ nan,~nan,~nan,~nan,~nan,~nan.} \NEWLINE
  \test:nn {nan} {nan}  \NEWLINE
  \test:nn {nan} {inf}  \NEWLINE
  \test:nn {nan} {-inf} \NEWLINE
  \test:nn {nan} {1}    \NEWLINE
  \test:nn {nan} {0}    \NEWLINE
  \test:nn {nan} {-0}   \NEWLINE
  \TYPE {Expect~ nan,~ error+nan,~ error+nan,~ inf,~ inf,~ -inf.} \NEWLINE
  \test:nn {inf} {nan}  \NEWLINE
  \test:nn {inf} {inf}  \NEWLINE
  \test:nn {inf} {-inf} \NEWLINE
  \test:nn {inf} {1}    \NEWLINE
  \test:nn {inf} {0}    \NEWLINE
  \test:nn {inf} {-0}   \NEWLINE
  \TYPE {Expect~ nan,~ error+nan,~ error+nan,~ -inf,~ -inf,~ inf.} \NEWLINE
  \test:nn {-inf} {nan} \NEWLINE
  \test:nn {-inf} {inf} \NEWLINE
  \test:nn {-inf} {-inf}\NEWLINE
  \test:nn {-inf} {1}   \NEWLINE
  \test:nn {-inf} {0}   \NEWLINE
  \test:nn {-inf} {-0}  \NEWLINE
  \TYPE {Expect~ nan,~ 0,~ -0,~ inf,~ -inf.} \NEWLINE
  \test:nn {1} {nan}    \NEWLINE
  \test:nn {1} {inf}    \NEWLINE
  \test:nn {1} {-inf}   \NEWLINE
  \test:nn {1} {0}      \NEWLINE
  \test:nn {1} {-0}     \NEWLINE
  \TYPE {Expect~ nan,~ -0,~ 0,~ -inf,~ inf.} \NEWLINE
  \test:nn {-1} {nan}    \NEWLINE
  \test:nn {-1} {inf}    \NEWLINE
  \test:nn {-1} {-inf}   \NEWLINE
  \test:nn {-1} {0}      \NEWLINE
  \test:nn {-1} {-0}     \NEWLINE
  \TYPE {Expect~ nan,~ +0,~ -0,~ +0,~ error+nan,~ error+nan.} \NEWLINE
  \test:nn {0} {nan}    \NEWLINE
  \test:nn {0} {inf}    \NEWLINE
  \test:nn {0} {-inf}   \NEWLINE
  \test:nn {0} {1}      \NEWLINE
  \test:nn {0} {0}      \NEWLINE
  \test:nn {0} {-0}     \NEWLINE
  \TYPE {Expect~ nan,~ -0,~ +0,~ -0,~ error+nan,~ error+nan.} \NEWLINE
  \test:nn {-0} {nan}   \NEWLINE
  \test:nn {-0} {inf}   \NEWLINE
  \test:nn {-0} {-inf}  \NEWLINE
  \test:nn {-0} {1}     \NEWLINE
  \test:nn {-0} {0}     \NEWLINE
  \test:nn {-0} {-0}    \NEWLINE
}

