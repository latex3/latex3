    \prop_put:Nnn \l_a_prop {} {}
    \prop_if_empty:NTF \l_a_prop {\TRUE} {\FALSE}
    \prop_put:Nnn \l_a_prop {abc} {}
    \prop_if_empty:NTF \l_a_prop {\ERROR} {\FALSE}
