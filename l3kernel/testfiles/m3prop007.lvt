    \prop_put:Nnn \l_a_prop { a } { # testa \par }
    \prop_put:Nnn \l_a_prop { \q_recursion_tail } { ? }
    \prop_put:Nnn \l_a_prop { b } { # testb \par }
    \cs_set:Npn \test:nn #1#2
      { \TYPE { > \tl_to_str:n {#1} --- \tl_to_str:n {#2} < } }
    \prop_map_function:NN \l_a_prop \test:nn
  }

