\TEST { clear }
  {
    \prop_put:Nnn \l_a_prop {tmp} { test~l \par }
    \prop_gput:Nnn \g_b_prop {tmp} { test~g \par }
    \prop_log:N \l_a_prop
    \prop_log:N \g_b_prop
    {
      \prop_clear:N  \l_a_prop
      \prop_gclear:N \g_b_prop
      \prop_log:N \l_a_prop
      \prop_log:N \g_b_prop
    }
    \prop_log:N \l_a_prop
    \prop_log:N \g_b_prop
  }

