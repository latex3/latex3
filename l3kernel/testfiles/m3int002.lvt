    \TYPE { \int_use:N \l_B_int }
  }
  \TYPE { \int_use:N \l_B_int }
  {
    \int_set_eq:Nc \l_B_int { l_A_int }
    \TYPE { \int_use:N \l_B_int }
  }
  \TYPE { \int_use:N \l_B_int }
  {
    \int_set_eq:cN { l_B_int } \l_A_int
