  \TIMO
  \int_while_do:nNnn { \l_A_int } < { 10 }
    {
      \message { [ \int_use:N \l_A_int ] }
      \int_incr:N \l_A_int
    }
  \SEPARATOR
  \OMIT
    \int_set:Nn \l_A_int { 0 }
  \TIMO
  \int_do_while:nNnn { \l_A_int } < { 10 }
    {
      \message { [ \int_use:N \l_A_int ] }
      \int_incr:N \l_A_int
    }
  \SEPARATOR
  \OMIT
    \int_set:Nn \l_A_int { 10 }
  \TIMO
  \int_while_do:nNnn { \l_A_int } < { 10 }
    {
      \message { [ \int_use:N \l_A_int ] }
