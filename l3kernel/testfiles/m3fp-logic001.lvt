\documentclass{minimal}
\input{regression-test}

\RequirePackage[enable-debug]{expl3}
\ExplSyntaxOn
\debug_on:n { check-declarations , deprecation , log-functions }
\ExplSyntaxOff

\begin{document}
\START
\AUTHOR{Bruno Le Floch}
\ExplSyntaxOn

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\OMIT
\cs_set:Npn \ { ~ }
\cs_new:Npn \test:nn #1#2
  {
    \fp_to_tl:n {#1} ~ and ~ \fp_to_tl:n {#2} \NEWLINE
    \ \
    \ \ \ \ \ \
    \ \ \ \ ? \
    \ \ \ > \ \
    \ \ \ > ? \
    \ \ = \ \ \
    \ \ = \ ? \
    \ \ = > \ \
    \ \ = > ? \
    \NEWLINE
    \ \
    \fp_compare:nTF { #1 !<=>? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nNnTF {#1}  ?  {#2} { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nNnTF {#1}  >  {#2} { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1    >? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1   =   #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1   = ? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1   =>  #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1   =>? #2 } { \ \ \TRUE } { \ \FALSE }
    \NEWLINE
    \ <
    \fp_compare:nTF { #1  <    #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1  <  ? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1  < >  #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1  < >? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1  <=   #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1  <= ? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1  <=>  #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1  <=>? #2 } { \ \ \TRUE } { \ \FALSE }
    \NEWLINE
    ! \
    \fp_compare:nTF { #1  <=>? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !   ? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !  >  #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !  >? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 ! =   #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 ! = ? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 ! =>  #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 ! =>? #2 } { \ \ \TRUE } { \ \FALSE }
    \NEWLINE
    !<
    \fp_compare:nTF { #1 !<    #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !<  ? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !< >  #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !< >? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !<=   #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !<= ? #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !<=>  #2 } { \ \ \TRUE } { \ \FALSE }
    \fp_compare:nTF { #1 !<=>? #2 } { \ \ \TRUE } { \ \FALSE }
    \NEWLINE
  }
\TIMO

\TESTEXP { Comparing~special~cases }
  {
    \test:nn {nan}  {nan}
    \test:nn {nan}  {inf}
    \test:nn {nan}  {-inf}
    \test:nn {nan}  {1}
    \test:nn {nan}  {0}
    \test:nn {nan}  {-0}
    \SEPARATOR\NEWLINE
    \test:nn {inf}  {nan}
    \test:nn {inf}  {inf}
    \test:nn {inf}  {-inf}
    \test:nn {inf}  {1}
    \test:nn {inf}  {0}
    \test:nn {inf}  {-0}
    \SEPARATOR\NEWLINE
    \test:nn {-inf} {nan}
    \test:nn {-inf} {inf}
    \test:nn {-inf} {-inf}
    \test:nn {-inf} {1}
    \test:nn {-inf} {0}
    \test:nn {-inf} {-0}
    \SEPARATOR\NEWLINE
    \test:nn {1}    {nan}
    \test:nn {1}    {inf}
    \test:nn {1}    {-inf}
    \test:nn {1}    {0}
    \test:nn {1}    {-0}
    \SEPARATOR\NEWLINE
    \test:nn {0}    {nan}
    \test:nn {0}    {inf}
    \test:nn {0}    {-inf}
    \test:nn {0}    {1}
    \test:nn {0}    {0}
    \test:nn {0}    {-0}
    \SEPARATOR\NEWLINE
    \test:nn {-0}   {nan}
    \test:nn {-0}   {inf}
    \test:nn {-0}   {-inf}
    \test:nn {-0}   {1}
    \test:nn {-0}   {0}
    \test:nn {-0}   {-0}
  }

\TESTEXP { Comparing~normal~floating~points }
  {
    \test:nn {1} {2}
    \test:nn {1} {1.000000000000001}
    \test:nn {1} {1}
    \test:nn {1} {0.999999999999999}
    \test:nn {1e23} {1e-23}
    \test:nn {-1} {1}
    \test:nn {-2} {-1}
    \test:nn {1} {-1}
  }

\TESTEXP { Comparing~tuples }
  {
    \test:nn {1} {()}
    \test:nn {()} {1}
    \test:nn {(1,-0)} {()}
    \test:nn {(1,-0)} {(1,+0)}
    \test:nn {(1,nan)} {(1,nan)}
    \test:nn {(2,3)} {(4,5)}
    \fp_compare:nTF { (1, 2) } { \TRUE } { \ERROR }
  }

\TESTEXP { Expansion~and~expressions }
  {
    \fp_to_tl:n { ( 2 = ~ \use_none:n { > } ? nan ) * 3 } \NEWLINE
    \fp_to_tl:n { ( 2 =< 3 ) <?> 1 } \NEWLINE
    \fp_to_tl:n { ( 2 =< 3 ) + \use:n { ( 10 } - 1 > 3 ) } \NEWLINE
    \fp_to_tl:n { ( 2 =< 3 ) !< ( 10 - 1 > 3 ) } \NEWLINE
  }

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\END
