      { \__msg_kernel_error:nnx { str } }
      { \use_none:nn }
  }
\cs_new_protected:Npn \@@_if_flag_no_error:nnx #1#2#3
  { \flag_if_raised:nT {#1} { \bool_gset_true:N \g_@@_error_bool } }
%    \end{macrocode}
% \end{macro}
% \end{macro}
