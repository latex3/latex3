    \char_category_name:n {  95 } , ~ % `\_
    \char_category_name:n {   9 } , ~ % `\^^J
    \char_category_name:n { 126 } , ~ % `\~
    \char_category_name:n {  97 } , ~ % `\a
    \char_category_name:n {  42 } , ~ % `\*
    \char_category_name:n {  63 } , ~ % `\?
