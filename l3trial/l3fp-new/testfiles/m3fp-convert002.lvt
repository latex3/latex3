\begin{document}
\START
\AUTHOR{Bruno Le Floch}
\ExplSyntaxOn

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TESTEXP{From~ integers}{
  \fp_use:n { \fp_from_int:n { 1 + 1*2 - 20/3 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { -1 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 0 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 1 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 12 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 123 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 1234 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 12345 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 123456 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 1234567 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 12345678 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 123456789 } } \NEWLINE
  \fp_use:n { \fp_from_int:n { 1234567890 } } \NEWLINE
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\TESTEXP{From~ dimensions}{
  \fp_use:n { \fp_from_dim:n { 1pt plus 1fil * 2 } } \NEWLINE
  \fp_use:n { \fp_from_dim:n { 0pt } } \NEWLINE
  \fp_use:n { \fp_from_dim:n { -1mm } } \NEWLINE
  \fp_use:n { \fp_from_dim:n { 10sp } } \NEWLINE
}

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\END
