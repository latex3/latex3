%   bigger than the second.
%    \begin{macrocode}
\prg_new_conditional:Npnn \bigint_compare:nNn #1#2#3 { p , T , F , TF }
  {
    \if_int_compare:w
        \@@_parse_do:nn {#3}
          { \@@_parse_do:nn {#1} \@@_compare:ww }
        #2 \c_zero_int
      \prg_return_true:
