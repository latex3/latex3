% \begin{macro}[EXP]{\@@_exp_after_o:w}
%    \begin{macrocode}
\cs_new:Npn \@@_exp_after_o:w
  { \@@_exp_after_f:nw { \exp_after:wN \exp_stop_f: } }
%    \end{macrocode}
% \end{macro}
