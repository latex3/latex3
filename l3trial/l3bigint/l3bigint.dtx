% \begin{macro}[EXP]{\@@_use_none_stop_f:n}
%   Get rid of the next token (typically~|1|) and stop
%   \texttt{f}-expansion.
%    \begin{macrocode}
\cs_new:Npn \@@_use_none_stop_f:n #1 { \exp_stop_f: }
%    \end{macrocode}
% \end{macro}
