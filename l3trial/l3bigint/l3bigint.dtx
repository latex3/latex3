% \begin{variable}{\c_@@_zero_bigint}
%   Used as an initial value for big integers.
%    \begin{macrocode}
\tl_const:Nn \c_@@_zero_bigint { \s_@@ \@@_chk:w 0 . 0 ; }
%    \end{macrocode}
% \end{variable}
