    \exp_after:wN #5
    \@@_int_value:w
    \exp_after:wN \@@_tadd_open:w \exp_after:wN +
    \@@_int_value:w
      \if_meaning:w #2 #3
        \exp_after:wN \@@_tadd_open:w \exp_after:wN -
      \else:
        \exp_after:wN \@@_tadd_open:w \exp_after:wN +
      \fi:
      0 . #1 + 0 . #5#6 . 0 . #4 .
    0 . #4 ; 0 . #5#6 ;
  }
\cs_new:Npn \@@_tmul_pp_auxiii:Nw #1 #2 +
  {
    \if_case:w #1 \exp_stop_f:
